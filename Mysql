--
-- Cấu trúc bảng cho bảng `tintuc`
--

CREATE TABLE `tintuc` (
  `id` int(11) NOT NULL,
  `image` text NOT NULL,
  `tieude` varchar(100) NOT NULL,
  `ten` varchar(200) NOT NULL,
  `mota` text NOT NULL,
  `noidung` text NOT NULL,
  `ngaydang` date NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

